@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="tr">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>AdminLTE 3 | Project Add</title>
    <!-- Responsive meta tag -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Font Awesome -->
    <link rel="stylesheet" href="/AdminLTE-3.0.4/plugins/fontawesome-free/css/all.min.css">

    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">

    <!-- AdminLTE CSS -->
    <link rel="stylesheet" href="/AdminLTE-3.0.4/dist/css/adminlte.min.css">

    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
</head>

<body class="hold-transition sidebar-mini">
    <div class="wrapper">
        <!-- Sidebar -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="/Default/Index/" class="brand-link">
                <img src="/AdminLTE-3.0.4/dist/img/AdminLTELogo.png" alt="AdminLTE Logo"
                     class="brand-image img-circle elevation-3" style="opacity: .8">
                <span class="brand-text font-weight-light">CV Sitesine Git </span>
            </a>

            <!-- Sidebar Menu -->
            <div class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="/AdminLTE-3.0.4/dist/img/user2-160x160.jpg" class="img-circle elevation-2" alt="User Image">
                    </div>
                    <div class="info">
                        <a href="#" class="d-block">Admin Paneli</a>
                    </div>
                </div>

                <!-- Navigation Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <li class="nav-item has-treeview">
                            <a href="/Hakkımda/Index" class="nav-link">
                                <i class="nav-icon fas fa-tachometer-alt"></i>
                                <p> Hakkımda </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Deneyim/Index/" class="nav-link">
                                <i class="nav-icon fas fa-th"></i>
                                <p> Deneyimlerim </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/Egitim/Index/" class="nav-link">
                                <i class="nav-icon fas fa-copy"></i>
                                <p> Eğitimim </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/Yeteneklerim/Index/" class="nav-link">
                                <i class="nav-icon fas fa-chart-pie"></i>
                                <p> Yeteneklerim </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/Sertifika/Index/" class="nav-link">
                                <i class="nav-icon fas fa-tree"></i>
                                <p> Sertifikalarım </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/Hobi/Index" class="nav-link">
                                <i class="nav-icon fas fa-edit"></i>
                                <p> Hobilerim </p>
                            </a>


                        <li class="nav-item has-treeview">
                            <a href="/İletisim/Index/" class="nav-link">
                                <i class="nav-icon fas fa-table"></i>
                                <p> İletişim </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/SosyalMedya/Index/" class="nav-link">
                                <i class="nav-icon fas fa-table"></i>
                                <p> Sosyal Medya </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/Hobi/Index" class="nav-link">
                                <i class="nav-icon fas fa-edit"></i>
                                <p> Hobilerim </p>
                            </a>
                        </li>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/Patron/LogOut/" class="nav-link">
                                <i class="nav-icon fas fa-table"></i>
                                <p> Çıkış </p>
                            </a>
                    </ul>
                </nav>
            </div>
        </aside>

        <!-- Content Wrapper -->
        <div class="content-wrapper">
            <!-- Main content -->
            <section class="content">
                <div class="row">
                    @RenderBody()
                </div>
            </section>
        </div>
    </div>

    <!-- jQuery -->
    <script src="/AdminLTE-3.0.4/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="/AdminLTE-3.0.4/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="/AdminLTE-3.0.4/dist/js/adminlte.min.js"></script>
</body>

</html>
