@using WebApplication3.Models.Entity
@model List<TblDeneyimlerim>
@foreach (var x in Model)
{
    <section class="resume-section" id="experience">
        <div class="resume-section-content">
            <h2 class="mb-5">Deneyİmlerİm</h2>
            <div class="d-flex flex-column flex-md-row justify-content-between mb-5">
                <div class="flex-grow-1">
                    <h3 class="mb-0">@x.Baslik</h3>
                    <div class="subheading mb-3">@x.AltBaslik</div>
                    <p>@x.Aciklama</p>
                </div>
                <div class="flex-shrink-0"><span class="text-primary">@x.Tarih - Devam Ediyor</span></div>
            </div>

        </div>
    </section>
    <hr class="m-0" />
}
