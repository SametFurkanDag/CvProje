@model WebApplication3.Models.Entity.TblSosyalMedya
@{
    ViewBag.Title = "SayfaGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}<br />
<h4>Sosyal Medya Düzenleme Sayfası</h4>
<br />
<form method="post" class="form-control">

    @Html.Label("Sosyal Medya Adı")
    <br />

    @Html.TextBoxFor(x => x.Ad, new { @class = "form-control" })
    <br />

    @Html.Label("Link")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("İkon")
    @Html.TextBoxFor(x => x.İkon, new { @class = "form-control" })
    <br />


    <button type="submit" class="btn btn-success">Güncelle</button>
</form>
